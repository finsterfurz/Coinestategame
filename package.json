{
  "name": "virtual-building-empire",
  "version": "2.1.1",
  "description": "Virtual Building Empire - Advanced Web3 Gaming Platform with Enhanced TypeScript Support",
  "keywords": [
    "gaming",
    "nft",
    "characters",
    "virtual-building",
    "lunc",
    "blockchain-game",
    "web3",
    "entertainment",
    "react",
    "typescript",
    "ethereum",
    "analytics",
    "ai",
    "quests",
    "social",
    "performance",
    "optimized"
  ],
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm start",
    "preview": "npm run build && serve -s build",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze:server": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js --mode server",
    "size": "npm run build && bundlesize",
    "size-limit": "npm run build && npx size-limit",
    "performance": "npm run build && npm run analyze && npm run size",
    "optimize": "npm run type-check && npm run lint:fix && npm run format && npm run build && npm run size",
    "security": "npm audit && npm run lint",
    "security:fix": "npm audit fix && npm run lint:fix",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=build",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "contracts:compile": "hardhat compile",
    "contracts:test": "hardhat test",
    "contracts:deploy": "hardhat run scripts/deploy.js",
    "contracts:verify": "hardhat verify",
    "node:start": "hardhat node",
    "build:production": "cross-env NODE_ENV=production npm run optimize",
    "clean": "rm -rf build node_modules/.cache",
    "clean:install": "npm run clean && npm install",
    "precommit": "npm run type-check && npm run lint:fix && npm run format",
    "prepush": "npm run test:coverage && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finsterfurz/Coinestategame.git"
  },
  "author": {
    "name": "finsterfurz",
    "email": "info@virtualbuilding.game",
    "url": "https://virtualbuilding.game"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.8.0",
    "ethers": "^5.7.2",
    "web3": "^4.0.0",
    "styled-components": "^5.3.0",
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.0.0",
    "recharts": "^2.5.0",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.0",
    "@tanstack/react-query": "^4.0.0",
    "lodash": "^4.17.21",
    "date-fns": "^2.29.0",
    "axios": "^1.3.0",
    "clsx": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-error-boundary": "^4.0.0",
    "react-intersection-observer": "^9.4.0",
    "react-lazyload": "^3.2.0",
    "uuid": "^9.0.0",
    "js-cookie": "^3.0.1",
    "country-flag-icons": "^1.5.0",
    "react-icons": "^4.7.0",
    "react-confetti": "^6.1.0",
    "react-toastify": "^9.1.0",
    "web3-utils": "^4.0.0",
    "dompurify": "^3.0.0",
    "socket.io-client": "^4.7.0",
    "react-spring": "^9.7.0",
    "react-use-gesture": "^9.1.3",
    "react-virtualized": "^9.22.0",
    "chart.js": "^4.0.0",
    "react-chartjs-2": "^5.0.0",
    "d3": "^7.8.0",
    "react-d3-graph": "^2.6.0",
    "three": "^0.155.0",
    "@react-three/fiber": "^8.0.0",
    "@react-three/drei": "^9.0.0",
    "react-redux": "^8.1.0",
    "@reduxjs/toolkit": "^1.9.0",
    "immer": "^10.0.0",
    "reselect": "^4.1.0",
    "react-dnd": "^16.0.0",
    "react-dnd-html5-backend": "^16.0.0",
    "react-hook-form": "^7.0.0",
    "yup": "^1.0.0",
    "@hookform/resolvers": "^3.0.0",
    "react-transition-group": "^4.4.0",
    "react-loading-skeleton": "^3.3.0",
    "react-window": "^1.8.0",
    "react-beautiful-dnd": "^13.1.0",
    "merkletreejs": "^0.3.0",
    "keccak256": "^1.0.0",
    "encoding": "^0.1.13"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/lodash": "^4.14.0",
    "@types/uuid": "^9.0.0",
    "@types/js-cookie": "^3.0.0",
    "@types/dompurify": "^3.0.0",
    "@types/d3": "^7.4.0",
    "@types/three": "^0.155.0",
    "@types/react-router-dom": "^5.3.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-transition-group": "^4.4.0",
    "@types/react-beautiful-dnd": "^13.1.0",
    "typescript": "^5.1.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^2.8.7",
    "webpack-bundle-analyzer": "^4.8.0",
    "bundlesize": "^0.18.0",
    "size-limit": "^8.2.0",
    "@size-limit/preset-app": "^8.2.0",
    "serve": "^14.2.0",
    "cross-env": "^7.0.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/user-event": "^14.4.0",
    "cypress": "^13.0.0",
    "start-server-and-test": "^2.0.0",
    "msw": "^1.3.0",
    "jest-environment-jsdom": "^29.0.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "source-map-explorer": "^2.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "homepage": "https://coinestategame.vercel.app",
  "bugs": {
    "url": "https://github.com/finsterfurz/Coinestategame/issues"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "800 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "250 kB"
    }
  ],
  "size-limit": [
    {
      "path": "build/static/js/*.js",
      "limit": "800 kB"
    },
    {
      "path": "build/static/css/*.css", 
      "limit": "250 kB"
    }
  ],
  "proxy": "http://localhost:3001",
  "private": false,
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test -- --coverage --watchAll=false --passWithNoTests",
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "@typescript-eslint",
      "react-hooks",
      "import",
      "jsx-a11y"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "import/order": ["error", {
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index"],
        "newlines-between": "always"
      }],
      "jsx-a11y/alt-text": "error",
      "jsx-a11y/aria-role": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 80,
        "statements": 80
      }
    }
  }
}