{
  "name": "virtual-building-empire",
  "version": "3.0.0",
  "description": "Enhanced Virtual Building Empire - Professional Web3 Gaming Platform with TypeScript, Advanced Testing, and Modern State Management",
  "keywords": [
    "gaming",
    "nft",
    "characters",
    "virtual-building",
    "lunc",
    "blockchain-game",
    "web3",
    "entertainment",
    "react",
    "typescript",
    "ethereum",
    "smart-contracts",
    "defi",
    "gamefi"
  ],
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --passWithNoTests",
    "test:ci": "react-scripts test --coverage --passWithNoTests --ci",
    "test:contracts": "hardhat test",
    "eject": "react-scripts eject",
    "dev": "npm start",
    "preview": "npm run build && serve -s build",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "size": "npm run build && bundlesize",
    "security": "npm audit && npm run lint",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=build",
    "compile": "hardhat compile",
    "deploy:contracts": "hardhat deploy",
    "node": "hardhat node",
    "gas-report": "REPORT_GAS=true hardhat test",
    "coverage:contracts": "hardhat coverage",
    "clean": "hardhat clean",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finsterfurz/Coinestategame.git"
  },
  "author": {
    "name": "finsterfurz",
    "email": "info@virtualbuilding.game",
    "url": "https://virtualbuilding.game"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.8.0",
    "ethers": "^6.10.0",
    "wagmi": "^2.5.0",
    "@wagmi/core": "^2.6.0",
    "viem": "^2.7.0",
    "@tanstack/react-query": "^5.25.0",
    "zustand": "^4.5.0",
    "immer": "^10.0.4",
    "styled-components": "^6.1.8",
    "lucide-react": "^0.343.0",
    "framer-motion": "^11.0.0",
    "recharts": "^2.12.0",
    "react-hot-toast": "^2.4.1",
    "lodash": "^4.17.21",
    "date-fns": "^3.6.0",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-error-boundary": "^4.0.13",
    "react-intersection-observer": "^9.8.1",
    "react-lazyload": "^3.2.0",
    "uuid": "^9.0.1",
    "js-cookie": "^3.0.5",
    "country-flag-icons": "^1.5.11",
    "react-icons": "^5.0.1",
    "react-confetti": "^6.1.0",
    "react-toastify": "^10.0.4",
    "web3-utils": "^4.2.1",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "react-i18next": "^14.0.5",
    "i18next": "^23.10.0",
    "react-hook-form": "^7.51.0",
    "@hookform/resolvers": "^3.3.4",
    "zod": "^3.22.4",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/lodash": "^4.17.0",
    "@types/uuid": "^9.0.8",
    "@types/js-cookie": "^3.0.6",
    "@types/react-helmet": "^6.1.11",
    "typescript": "^5.3.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-config-prettier": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "prettier": "^3.2.5",
    "webpack-bundle-analyzer": "^4.10.1",
    "bundlesize": "^0.18.1",
    "serve": "^14.2.1",
    "cross-env": "^7.0.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/react-hooks": "^8.0.1",
    "hardhat": "^2.21.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/hardhat-upgrades": "^3.0.5",
    "hardhat-gas-reporter": "^2.0.2",
    "solidity-coverage": "^0.8.11",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "cypress": "^13.7.1",
    "@cypress/react": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "homepage": "https://coinestategame.vercel.app",
  "bugs": {
    "url": "https://github.com/finsterfurz/Coinestategame/issues"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "1 MB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "300 kB"
    }
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/types/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "proxy": "http://localhost:3001",
  "private": false
}