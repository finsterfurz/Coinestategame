{
  "name": "virtual-building-empire",
  "version": "2.0.0",
  "description": "Virtual Building Empire - Modern Web3 Gaming Platform with NFT Characters and LUNC Rewards",
  "keywords": [
    "gaming",
    "nft",
    "characters",
    "virtual-building",
    "lunc",
    "blockchain-game",
    "web3",
    "entertainment",
    "react",
    "ethereum"
  ],
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm start",
    "preview": "npm run build && serve -s build",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "size": "npm run build && bundlesize",
    "security": "npm audit && npm run lint",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finsterfurz/Coinestategame.git"
  },
  "author": {
    "name": "finsterfurz",
    "email": "info@virtualbuilding.game",
    "url": "https://virtualbuilding.game"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.8.0",
    "ethers": "^5.7.2",
    "web3": "^4.0.0",
    "styled-components": "^5.3.0",
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.0.0",
    "recharts": "^2.5.0",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.0",
    "lodash": "^4.17.21",
    "date-fns": "^2.29.0",
    "axios": "^1.3.0",
    "clsx": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-error-boundary": "^4.0.0",
    "react-intersection-observer": "^9.4.0",
    "react-lazyload": "^3.2.0",
    "uuid": "^9.0.0",
    "js-cookie": "^3.0.1",
    "country-flag-icons": "^1.5.0",
    "react-icons": "^4.7.0",
    "react-confetti": "^6.1.0",
    "react-toastify": "^9.1.0",
    "web3-utils": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/lodash": "^4.14.0",
    "@types/uuid": "^9.0.0",
    "@types/js-cookie": "^3.0.0",
    "typescript": "^4.9.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-config-prettier": "^8.7.0",
    "prettier": "^2.8.7",
    "webpack-bundle-analyzer": "^4.8.0",
    "bundlesize": "^0.18.0",
    "serve": "^14.2.0",
    "cross-env": "^7.0.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/user-event": "^14.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "homepage": "https://coinestategame.vercel.app",
  "bugs": {
    "url": "https://github.com/finsterfurz/Coinestategame/issues"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "1 MB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "300 kB"
    }
  ],
  "proxy": "http://localhost:3001",
  "private": false
}