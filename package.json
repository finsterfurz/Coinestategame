{
  "name": "virtual-building-empire",
  "version": "1.0.0",
  "description": "Virtual Building Empire - Character Collection Game with LUNC Rewards",
  "keywords": [
    "gaming",
    "nft",
    "characters",
    "virtual-building",
    "lunc",
    "blockchain-game",
    "web3",
    "dubai-llc",
    "entertainment"
  ],
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm start",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:goerli": "hardhat run scripts/deploy.js --network goerli",
    "deploy:polygon": "hardhat run scripts/deploy.js --network polygon",
    "deploy:bsc": "hardhat run scripts/deploy.js --network bsc",
    "test:contracts": "hardhat test",
    "coverage": "hardhat coverage",
    "node": "hardhat node",
    "verify": "hardhat verify",
    "gas-report": "REPORT_GAS=true hardhat test",
    "clean": "hardhat clean",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "format": "prettier --write src/",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finsterfurz/Coinestategame.git"
  },
  "author": "finsterfurz",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.8.0",
    "ethers": "^5.7.2",
    "web3": "^4.0.0",
    "styled-components": "^5.3.0",
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.0.0",
    "recharts": "^2.5.0",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.0",
    "lodash": "^4.17.21",
    "date-fns": "^2.29.0",
    "react-spring": "^9.6.1",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^4.9.0",
    "hardhat": "^2.14.0",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.9.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "chai": "^4.3.7",
    "ethereum-waffle": "^4.0.10",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "homepage": "https://virtualbuilding.game",
  "bugs": {
    "url": "https://github.com/finsterfurz/Coinestategame/issues"
  }
}